  pod 'GooglePlaces'
  pod 'GooglePlacePicker'
  pod 'GoogleMaps'

  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for backalla

  target 'backallaTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'backallaUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
